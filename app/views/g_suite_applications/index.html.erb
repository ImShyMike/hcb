<p id="notice"><%= notice %></p>

<h1>G Suite Applications</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Event</th>
      <th>User</th>
      <th>Domain</th>
      <th>Rejected at</th>
      <th>Accepted at</th>
      <th>Canceled at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @g_suite_applications.each do |g_suite_application| %>
      <tr>
        <td><%= g_suite_application.user %></td>
        <td><%= g_suite_application.event %></td>
        <td><%= g_suite_application.user %></td>
        <td><%= g_suite_application.domain %></td>
        <td><%= g_suite_application.rejected_at %></td>
        <td><%= g_suite_application.accepted_at %></td>
        <td><%= g_suite_application.canceled_at %></td>
        <td><%= link_to 'Show', g_suite_application %></td>
        <td><%= link_to 'Edit', edit_g_suite_application_path(g_suite_application) %></td>
        <td><%= link_to 'Destroy', g_suite_application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New G Suite Application', new_g_suite_application_path %>
