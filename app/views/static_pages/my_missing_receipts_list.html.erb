<% if @missing %>
  <section class="card p0 mt3 mb3">
    <h2 class="heading h2 line-height-3 mt0 ml0 pt1 pb1 pl2 pr2">
      Charges missing receipts
      <%= link_to my_inbox_path, class: 'btn' do %>
        <%= inline_icon 'view' %>
        See all
      <% end %>
    </h2>
    <ul>
    <% @missing.each do |missing| %>
      <%= render partial: "canonical_transactions/canonical_transaction", locals: { ct: missing } %>
    <% end %>
    </ul>
  </section>
<% end %>
