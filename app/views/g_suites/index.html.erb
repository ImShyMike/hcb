<p id="notice"><%= notice %></p>

<h1>G Suites</h1>

<table>
  <thead>
    <tr>
      <th>G Suite application</th>
      <th>Domain</th>
      <th>Event</th>
      <th>Verification key</th>
      <th>Deleted at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @g_suites.each do |g_suite| %>
      <tr>
        <td><%= g_suite.g_suite_application %></td>
        <td><%= g_suite.domain %></td>
        <td><%= g_suite.event %></td>
        <td><%= g_suite.verification_key %></td>
        <td><%= g_suite.deleted_at %></td>
        <td><%= link_to 'Show', g_suite %></td>
        <td><%= link_to 'Edit', edit_g_suite_path(g_suite) %></td>
        <td><%= link_to 'Destroy', g_suite, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New G Suite', new_g_suite_path %>
