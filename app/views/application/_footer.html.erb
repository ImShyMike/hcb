<p class="center mt4 muted">
<% if current_user %>
  <% if organizer_signed_in? %>
    <%= help_message %>
  <% end %>
  </p>

  <p class="center muted">
<% end %>
  <%= link_to current_user && organizer_signed_in? ? 'Bank FAQs' : 'Hack Club Bank FAQs', faq_path, class: 'muted' %>
  <% if organizer_signed_in? %>
    <span class="inline-block pl1 pr1">&middot;</span>
    <%= link_to 'Brand Guidelines', branding_path, class: 'muted' %>
  <% end %>
</p>

<% unless commit_hash.blank? %>
  <p class="center muted h6">
    <% if commit_hash and commit_duration %>
      Build <em><%= commit_hash %></em> from <%= commit_duration %> ago.
    <% elsif commit_hash %>
      Build <em><%= commit_hash %></em>.
    <% end %>
  </p>
<% end %>
