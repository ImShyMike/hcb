<h1>G Suite Accounts</h1>

<table>
  <thead>
    <tr>
      <th>G Suite email</th>
      <th>Backup email</th>
      <th>Accepted at</th>
      <th>Rejected at</th>
      <th>G suite</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @g_suite_accounts.each do |g_suite_account| %>
      <tr>
        <td><%= g_suite_account.address %></td>
        <td><%= g_suite_account.backup_email %></td>
        <td><%= g_suite_account.accepted_at %></td>
        <td><%= g_suite_account.rejected_at %></td>
        <td><%= link_to 'G Suite', g_suite_account.g_suite %></td>
        <td><%= link_to 'Edit', edit_g_suite_account_path(g_suite_account) %></td>
        <td><%= link_to 'Destroy', g_suite_account, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
